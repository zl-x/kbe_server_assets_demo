<root>
    <Properties>
		<roleType>
			<Type>			INT32		    </Type>

			<!--
			    Flags 只是表明哪些 entity 可以接收到这个属性, 在服务端更改后, 客户端是否会接收到通知
			    客户端并没有主动更改然后通知服务端的底层 Flags 设置, 都需要通过 base 或 cell 方法
			 -->

			<Flags>			ALL_CLIENTS	    </Flags>
			<Persistent>	true		    </Persistent>
		</roleType>

		<level>
			<Type>			INT32		            </Type>
            <Flags>			ALL_CLIENTS		        </Flags>
			<Persistent>	true		            </Persistent>
		</level>

		<name>
			<Type>			UNICODE		</Type>
            <DatabaseLength> 32			</DatabaseLength>

			<Flags>			ALL_CLIENTS	</Flags>
			<Persistent>	true		</Persistent>

            <!-- 属性的索引类别，UNIQUE, INDEX，分别对应唯一索引、普通索引 -->
		    <!-- <Index> 		UNIQUE 		</Index> -->
		</name>

		<Addr_Id>
			<Type>			INT32		            </Type>
            <Flags>			OWN_CLIENT		        </Flags>
			<Persistent>	true		            </Persistent>
		</Addr_Id>

    </Properties>
    <!-- BaseApp上的远程方法 -->
    <BaseMethods>
    </BaseMethods>
    <!-- CellApp上的远程方法 -->
    <CellMethods>
    </CellMethods>
</root>