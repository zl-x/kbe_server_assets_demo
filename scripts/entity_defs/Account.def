<root>
	<Properties>
		<test_val>
			<Type>	TEST_STRUCT	</Type>
			<Flags>	BASE_AND_CLIENT	</Flags>
			<Persistent>	true	</Persistent>
		</test_val>

		<test_val_list>
			<Type>	TEST_STRUCT_LIST	</Type>
			<Flags>	BASE_AND_CLIENT	</Flags>
			<Persistent>	true	</Persistent>
		</test_val_list>

        <characters>
			<Type>			AVATAR_DESCRIBE_LIST	</Type>
			<Flags>			BASE			        </Flags>
			<Persistent>		true				</Persistent>
        </characters>

        <lastSelCharacter>
			<Type>			DBID					</Type>
			<Flags>			BASE_AND_CLIENT			</Flags>
			<Default>		0						</Default>
			<Persistent>		true				</Persistent>
		</lastSelCharacter>

	</Properties>

	<ClientMethods>
		<sayHelloOnClient>
			<Arg>   INT32         </Arg>
			<Arg>   UNICODE         </Arg>
			<Arg>   INT32         </Arg>
        </sayHelloOnClient>

        <reqAvatarListResultOnClient>
            <Arg>   AVATAR_DESCRIBE_LIST         </Arg>
		</reqAvatarListResultOnClient>

		<reqCreateAvatarResultOnClient>
			<Arg>			INT32				</Arg>	<!-- result -->
			<Arg>       AVATAR_DESCRIBE         </Arg>
		</reqCreateAvatarResultOnClient>

		<reqRemoveAvatarResultOnClient>
			<Arg>			INT32				</Arg>	<!-- result -->
		</reqRemoveAvatarResultOnClient>

	</ClientMethods>

	<BaseMethods>
		<sayHelloOnBase>
			<Exposed/>
			<Arg>   INT32         </Arg>
			<Arg>   UNICODE         </Arg>
			<Arg>   INT32         </Arg>
        </sayHelloOnBase>

        <reqAvatarListOnBase>
			<Exposed/>
		</reqAvatarListOnBase>

		<reqCreateAvatarOnBase>
			<Exposed/>
			<Arg>			INT32				</Arg>	<!-- roleType -->
			<Arg>			UNICODE				</Arg>	<!-- name 为什么这里不需要长度定义??-->
		</reqCreateAvatarOnBase>

		<reqRemoveAvatarOnBase>
			<Exposed/>
			<Arg>			DBID				</Arg>	<!-- dbid -->
		</reqRemoveAvatarOnBase>

		<selectAvatarEnterGameOnBase>
			<Exposed/>
			<Arg>			DBID				</Arg>	<!-- dbid -->
		</selectAvatarEnterGameOnBase>

	</BaseMethods>

	<CellMethods>
	</CellMethods>

</root>
